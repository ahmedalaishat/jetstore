<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.alaishat.ahmed.mobostore">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".MoboStoreApp"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MoboStore"
        tools:targetApi="31">
        <activity
            android:name="com.alaishat.ahmed.mobostore.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.MoboStore">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.alaishat.ahmed.mobostore.androidx-startup"
            android:exported="false"
            tools:node="merge">

            <meta-data
                android:name="com.alaishat.ahmed.mobostore.initializer.TimberInitializer"
                android:value="androidx.startup" />
        </provider>

    </application>

</manifest>